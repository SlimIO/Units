{
  "name": "@slimio/units",
  "version": "0.3.1",
  "description": "SlimIO Metrics Units",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "private": false,
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Units.git"
  },
  "keywords": [
    "SlimIO",
    "Units",
    "Metrics Type"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/is/issues"
  },
  "homepage": "https://github.com/SlimIO/is#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.2",
    "@slimio/psp": "^0.6.0",
    "ava": "^2.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "jsdoc": "^3.6.2",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/lazy": "^1.2.0"
  }
}
